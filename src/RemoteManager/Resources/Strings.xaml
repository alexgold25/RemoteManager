<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=System.Runtime">

    <!-- Column headers -->
    <sys:String x:Key="Agents.Header.Status">Status</sys:String>
    <sys:String x:Key="Agents.Header.Name">Name</sys:String>
    <sys:String x:Key="Agents.Header.Host">Host/IP</sys:String>
    <sys:String x:Key="Agents.Header.Group">Group/Tags</sys:String>
    <sys:String x:Key="Agents.Header.Version">Version</sys:String>
    <sys:String x:Key="Agents.Header.Uptime">Uptime</sys:String>
    <sys:String x:Key="Agents.Header.Platform">Platform</sys:String>
    <sys:String x:Key="Agents.Header.CpuRam">CPU/RAM</sys:String>
    <sys:String x:Key="Agents.Header.Ping">Ping</sys:String>
    <sys:String x:Key="Agents.Header.LastSeen">Last Seen</sys:String>
    <sys:String x:Key="Agents.Header.Fingerprint">Fingerprint</sys:String>
    <sys:String x:Key="Agents.Header.Actions">Actions</sys:String>

    <!-- Filters -->
    <sys:String x:Key="Agents.Filter.All">All</sys:String>
    <sys:String x:Key="Agents.Filter.Online">Online</sys:String>
    <sys:String x:Key="Agents.Filter.Offline">Offline</sys:String>
    <sys:String x:Key="Agents.Filter.Pending">Pending</sys:String>
    <sys:String x:Key="Agents.Filter.Quarantine">Quarantine</sys:String>

    <!-- Actions -->
    <sys:String x:Key="Agents.Action.OpenDetails">Open Details</sys:String>
    <sys:String x:Key="Agents.Action.Trust">Trust</sys:String>
    <sys:String x:Key="Agents.Action.Reject">Reject</sys:String>
    <sys:String x:Key="Agents.Action.Files">Files</sys:String>
    <sys:String x:Key="Agents.Action.Terminal">Terminal</sys:String>
    <sys:String x:Key="Agents.Action.Restart">Restart</sys:String>

    <!-- Search -->
    <sys:String x:Key="Agents.Search.Placeholder">Search agents...</sys:String>
    <!-- Это кодпоинт MDL2; в потребляющем контроле укажи FontFamily='Segoe MDL2 Assets' -->
    <sys:String x:Key="Icon.Search">&#xE721;</sys:String>

    <!-- Empty state -->
    <sys:String x:Key="Agents.Empty.Message">No agents found</sys:String>
    <sys:String x:Key="Agents.Empty.ScanNetwork">Scan network</sys:String>
    <sys:String x:Key="Agents.Empty.AddManual">Add manually</sys:String>

    <!-- Details -->
    <sys:String x:Key="Agents.Details.DeviceId">DeviceId</sys:String>
    <sys:String x:Key="Agents.Details.Hostname">Hostname</sys:String>
    <sys:String x:Key="Agents.Details.IPs">IPs</sys:String>
    <sys:String x:Key="Agents.Details.Tags">Tags</sys:String>
    <sys:String x:Key="Agents.Details.Fingerprint">Fingerprint</sys:String>
    <sys:String x:Key="Agents.Details.TrustStatus">Trust Status</sys:String>
    <sys:String x:Key="Agents.Details.Cert">Cert</sys:String>
    <sys:String x:Key="Agents.Details.Subject">Subject</sys:String>
    <sys:String x:Key="Agents.Details.NotBefore">Not Before</sys:String>
    <sys:String x:Key="Agents.Details.NotAfter">Not After</sys:String>
    <sys:String x:Key="Agents.Details.SAN">SAN</sys:String>
    <sys:String x:Key="Agents.Details.Uptime">Uptime</sys:String>
    <sys:String x:Key="Agents.Details.Version">Version</sys:String>
    <sys:String x:Key="Agents.Details.LastErrors">Last Errors</sys:String>
    <sys:String x:Key="Agents.Details.TrustBtn">Trust</sys:String>
    <sys:String x:Key="Agents.Details.RejectBtn">Reject</sys:String>
    <sys:String x:Key="Agents.Details.QuarantineBtn">Quarantine</sys:String>
    <sys:String x:Key="Agents.Details.RenameBtn">Rename</sys:String>
    <sys:String x:Key="Agents.Details.AssignTagsBtn">Assign Tags</sys:String>
    <sys:String x:Key="Agents.Details.RestartBtn">Restart Agent</sys:String>
    <sys:String x:Key="Agents.Details.CloseBtn">Close</sys:String>

</ResourceDictionary>
