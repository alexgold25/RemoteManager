{
  "Server": {
    "Enabled": true,
    "BindIp": "0.0.0.0",
    "Port": 8443,
    "EnableUdpQuic": true
  },
  "Discovery": {
    "Enabled": true,
    "ReceiveEnabled": true,
    "SendEnabled": true,
    "StrictSigned": true
  },
  "Client": {
    "DefaultAgentEndpoint": "https://127.0.0.1:8443"
  },
  "Security": {
    "Mode": "Auto",
    "DevCertPath": "certs/dashboard.pfx",
    "DevCertPassword": "dev",
    "PinningRequired": "Strict",
    "DiscoveryPsk": "dev-psk-please-change",
    "AutoTrust": {
      "Subnets": ["10.10.0.0/16"],
      "SubjectRegex": ""
    },
    "Certs": {
      "ExpiryWarnDays": 30
    }
  },
  "Ui": {
    "Theme": "Dark",
    "ShowCardModeByDefault": false
  },
  "Paths": {
    "TrustedAgents": "%AppData%/RemoteManager/trusted_agents.json"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [ { "Name": "File", "Args": { "path": "logs/dashboard-.log", "rollingInterval": "Day" } } ]
  }
}
