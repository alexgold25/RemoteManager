{
  "Server": {
    "Enabled": false,
    "BindIp": "127.0.0.1",
    "Port": 8443,
    "EnableUdpQuic": false
  },
  "Discovery": {
    "Enabled": false,
    "ReceiveEnabled": false,
    "SendEnabled": false,
    "StrictSigned": false
  },
